{
  "extends": [
    "tslint-config-airbnb"
  ],
  "rules": {
    "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"],
    "strict-boolean-expressions": [false],
    "max-line-length": [true, 120]
  },
  "linterOptions": {
    "exclude": [
      "**/src/app/shared/**"
    ]
  }
}
